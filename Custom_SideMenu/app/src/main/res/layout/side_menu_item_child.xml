<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_sub_item_wrapper"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#efefef"
    android:padding="@dimen/side_nav_padding"
    android:paddingStart="30dp">

    <TextView
        android:id="@+id/child_item_bullet_point"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="5dp"
        android:text="-"
        android:textSize="@dimen/nav_child_txt" />

    <TextView
        android:id="@+id/child_item_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@id/child_item_bullet_point"
        android:textSize="@dimen/nav_child_txt"
        tools:text="조회" />
</RelativeLayout>